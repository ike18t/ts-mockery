  "bugs": {
    "url": "https://github.com/ike18t/ts-mockery/issues"
  },
  "homepage": "https://github.com/ike18t/ts-mockery#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/jest": "^23.1.1",
    "@types/node": "^8.10.20",
    "istanbul": "^0.4.5",
    "jasmine-ts": "^0.2.1",
    "jest": "^23.1.0",
    "karma": "^2.0.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
