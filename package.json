    "test:jasmine": "karma start",
    "test:jest": "jest",
    "test": "npm run test:jasmine && npm run test:jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ike18t/ts-mockery.git"
  },
